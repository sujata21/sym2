{% trans_default_domain 'FOSUserBundle' %}

<h4>Add new user</h4>
<form id="formNewUser" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <button id="btnNewUserSave" type="submit">Save</button>
        <button id="btnNewEditCancel" type="button">Cancel</button>
    </div>
</form>
