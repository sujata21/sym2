{% extends "AWSonyScriptBundle::layout.html.twig" %}

{% block body -%}
    <h1>Projects</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Template</th>
                <td>{{ entity.template }}</td>
            </tr>
            <tr>
                <th>Alias</th>
                <td>{{ entity.alias }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ entity.number }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Spreadsheet</th>
                <td>{{ entity.spreadsheet }}</td>
            </tr>
            <tr>
                <th>Spreadsheet</th>
                <td>{{ entity.spreadsheetCopy }}</td>
            </tr>
        </tbody>
    </table>

{#
{% set i=1 %}
