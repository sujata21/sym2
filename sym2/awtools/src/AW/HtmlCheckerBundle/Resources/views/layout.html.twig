<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% stylesheets
        'bundles/mainassets/bootstrap3/css/bootstrap.min.css'
        'bundles/mainassets/bootstrap3/css/bootstrap-theme.min.css'
        'bundles/mainassets/spellchecker/css/jquery.spellchecker.min.css'
        'bundles/awhtmlchecker/css/*'
        filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <title>HTML Profanity & Spam Checker{% block title %}{% endblock %}</title>
</head>
<body>
    {% block body %}{% endblock %}
    {% javascripts 
        '@MainAssetsBundle/Resources/public/jquery/jquery-1.10.1.min.js'
        '@MainAssetsBundle/Resources/public/bootstrap3/js/bootstrap.min.js'
        '@AWHtmlCheckerBundle/Resources/public/js/*'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="text/javascript" src="/bundles/mainassets/spellchecker/js/jquery.spellchecker.min.js"></script>
    {% block bottom_javascripts %}{% endblock %}
</body>
</html>