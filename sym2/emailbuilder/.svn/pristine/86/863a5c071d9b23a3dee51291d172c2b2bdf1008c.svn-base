<style type="text/css">

div, p, a, li, td { -webkit-text-size-adjust:none;-ms-text-size-adjust:none;}   
body { margin: 0;	padding: 0;	background-color: #ebebeb;}      
table td {border-collapse:collapse;}    
.externalclass {width:100%;}    
.externalclass * {line-height: 100%}

a[href^="tel"], a[href^="sms"] {
	color: inherit !important;
	cursor: default !important;
	text-decoration:none !important;
}


@media only screen and (max-width: 420px) {

table[class="hide"], img[class="hide"], td[class="hide"], br[class="hide"], span[class="hide"] {
	display:none !important;
}

*[class].show {
  	display:block !important; 
  	overflow:visible !important; 
  	float:none !important;  
  	width:100%; 
  	height:auto !important; 
}

table[class="fluid"], td[class="fluid"], img[class="fluid"] {
  width:100% !important;
  height:auto !important;
  display:table !important; 
} 


body .block {
	display: block !important;
}

body .tbl100 {
	width: 100% !important;
}

table[class=w320], td[class=w320], div[class=w320], img[class=w320]{
  	width:320px !important;
  	height:auto !important;
} 
table[class=w300], td[class=w300], div[class=w300], img[class=w300]{
  	width:300px !important;
  	height:auto !important;
} 
table[class=w280], td[class=w280], div[class=w280], img[class=w280]{
  	width:280px !important;
  	height:auto !important;
} 
table[class=w280fs20], td[class=w280fs20], div[class=w280fs20], img[class=w280fs20]{
